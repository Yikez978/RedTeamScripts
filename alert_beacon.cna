
on beacon_initial {
        local('$host $ip $os $ver');
        $host = beacon_info($1, "computer");
        $ip = beacon_info($1, "internal");
        $os = beacon_info($1, "os");
        $ver = beacon_info($1, "ver");
        exec("/root/RedTeamScripts/alert_beacon.py --host " . $host . " --ip " . $ip . " --os " . $os . " --version " . $ver);
}
